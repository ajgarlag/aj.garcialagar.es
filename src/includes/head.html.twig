<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{ page.description | default(site.description) }}">
    <meta name="generator" content="Spress v{{ spress.version }}">
    {% if page.location %}<meta http-equiv="refresh" content="0;URL='{{page.location}}'" />{% endif %}
    <link rel="home" type="application/rss+xml" href="{{ site.url }}/rss.xml" />
    {% if page.generator is not defined %}
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:site" content="@ajgarlag" />
    <meta name="twitter:title" content="{{ page.title ? page.title : site.title }}" />
    <meta name="twitter:description" content="{{ page.description | default(site.description) }}" />
    <meta name="twitter:image" content="{{ site.url }}/{{ page.header_img.url | default(site.header_img.url) }}" />
    {% endif %}

    <title>{{ page.title ? page.title ~ ' | ' ~ site.title : site.title }} - Antonio J. García Lagar</title>

    <link rel="canonical" href="{{ site.url ~ page.url | replace({'index.html' : ''}) }}">

    <!-- Bootstrap Core CSS -->
